  "private": true,
  "scripts": {
    "android": "ENVFILE=.env.develop react-native run-android --variant 'developDebug'",
    "ios": "ENVFILE=.env.develop react-native run-ios --scheme 'treemapper(.env.develop)'",
    "android:staging": "ENVFILE=.env.staging react-native run-android --variant 'stagingDebug'",
    "ios:staging": "ENVFILE=.env.staging react-native run-ios --scheme 'treemapper(.env.staging)'",
